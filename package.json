{
  "name": "segment",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:browser && npm run test:node",
    "test:browser": "semistandard src test && browserify ./test/index.js -t [ babelify ] | browser-run | tap-spec",
    "test:node": "babel-node ./test/index.js | tap-spec",
    "compile": "npm run compile:browser && npm run compile:node",
    "compile:node": "babel src -d dist/node --optional runtime",
    "compile:browser": "browserify ./src/index.js -o dist/main.js -t [babelify --optional runtime] --standalone universal"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "babelify": "^6.2.0",
    "browserify": "^11.0.1"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-runtime": "^5.8.25",
    "isomorphic-fetch": "^2.1.1",
    "node-fetch": "^1.3.3"
  }
}
